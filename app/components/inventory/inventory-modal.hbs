{{!-- --}}
<nav>
  <div class="nav nav-tabs border-bottom-0 mx-auto my-5 d-flex justify-content-center" id="nav-tab" role="tablist">
    {{#each this.TabNames as |tab index|}}
    <button class="nav-item nav-link {{if (eq index 0) 'active'}} px-3" id="nav-srd-product-{{index}}" data-toggle="tab"
      data-target="#nav-srd-product-target-{{index}}" type="button" role="tab" 
      {{on 'click' (fn this.handleTabName tab)}} aria-controls="nav-srd-product-target-{{index}}" aria-selected="{{if (eq index 0) 'true' 'false'}}">
      <div class="m-0">
        {{tab}}
      </div>
    </button>
    {{/each}}
  </div>
</nav>



<div class="tab-content" id="nav-tabContent  {{if (eq this.selectedTab 'Inventory') 'active'}}">
  {{!-- {{#each this.TabNames as |tabContent index|}} --}}
    {{!-- <div class="tab-pane fade {{if (eq index 0) 'show active'}} " id="nav-srd-product-target-{{index}}" role="tabpanel" aria-labelledby="nav-srd-product-{{index}}"> --}}
      {{#if (eq this.selectedTab "Inventory")}}
        {{! create Inventory Buttons }}
        <section class="my-4 d-flex justify-content-start container-fluid w-100 table-responsive table-container p-0">
          <div class="flex">
            <div class="mx-auto text-center mt-3">
              <button
                type="button"
                class="btn btn-primary btn-lg"
                onclick={{fn @changeInventoryType "inventory-list"}}
              >
                Create Inventory
              </button>
            </div>
          </div>
        </section>

        <Inventory::Createinventorytable 
        @selectedTab={{this.selectedTab}}
        @handlecreateinventoryedit={{@handlecreateinventoryedit}}
        @columns={{@columns}} @items={{@items}}
        @handleInventoryStatusEdit={{@handleInventoryStatusEdit}}
        />
      {{else if (eq this.selectedTab "Inventory type")}}
        <Inventory::InventoryType />
      {{else if (eq this.selectedTab "Inventory Catagory type")}}
        <Inventory::InventoryCatagoryType />
      {{else if (eq this.selectedTab "Inventory Catagory")}}
        <Inventory::InventoryCatagory />
      {{else if (eq this.selectedTab "Storage type")}}
        <Inventory::StorageType />
      {{else if (eq this.selectedTab "Allocation type")}}
        <Inventory::AllocationType />
      {{else if (eq this.selectedTab "Unit of Measure")}}
        <Inventory::UnitOfMeasure />
      {{/if}}
    {{!-- </div> --}}
  {{!-- {{/each}} --}}
</div>



